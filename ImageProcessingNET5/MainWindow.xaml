        <Label Name="lblBytes" Content="Label" HorizontalAlignment="Left" Margin="304,217,0,0" VerticalAlignment="Top"/>
        <Label Name="lblGrid" Content="Label" HorizontalAlignment="Left" Margin="514,217,0,0" VerticalAlignment="Top"/>
        <Label Name="lblMatrix" Content="Label" HorizontalAlignment="Left" Margin="744,217,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblIgualdade" Content="" HorizontalAlignment="Left" Margin="10,243,0,0" VerticalAlignment="Top"/>
